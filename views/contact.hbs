
<div class="row">
  <div class="col-md-6">
    <form id="messageDudeForm" method="POST">
      <div class="form-group">
        <label for="nameInput">Name</label>
        <input type="text" class="form-control" id="nameInput" name='name' placeholder="Jane Smith">
      </div>
      <div class="form-group">
        <label for="emailInput">Email address</label>
        <input type="email" class="form-control" id="emailInput" name='email' placeholder="name@example.com">
      </div>
      <div class="form-group">
        <label for="messageDudeTextarea">Message to the Dude</label>
        <textarea class="form-control" id="messageDudeTextarea" name='message' rows="7"></textarea>
      </div>
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>

  </div>
  <div class="col-md-6">
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
      <div class="carousel-inner">
        {{#each instafeed}}
        <div class="carousel-item {{#if active}}active{{/if}}">
          <img class="d-block" src="{{images.standard_resolution.url}}" alt="First slide">
        </div>
        {{/each}}
      </div>
      <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>


  </div>
</div>
